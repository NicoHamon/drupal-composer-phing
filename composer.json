{
    "name": "dropteam drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        },
        {
            "type": "package",
            "package": {
                "name": "pfrenssen/phpcs-pre-push",
                "version": "1.0",
                "type": "git-hook",
                "source": {
                    "url": "https://gist.github.com/498fc52fea3f965f6640.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://git.drupal.org/project/phingdrushtask.git"
        }
    ],
    "require": {
        "composer/composer": "~1.0",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "~1.0",
        "drupal/core": "~8.1",
        "drush/drush": "~8.0",
        "php": ">=5.5.9",
        "drupal/admin_menu": "8.x",
        "drupal/adminimal_theme": "~8.1",
        "drupal/adminimal_admin_toolbar": "~8.1",
        "drupal/admin_toolbar": "~8.1",
        "drupal/better_formats": "~8.1",
        "drupal/boolean_formatter": "~8.1",
        "drupal/bootstrap": "~8.3",
        "drupal/bootstrap_layouts": "~8.3",
        "drupal/bootstrap_library": "~8.1",
        "drupal/config_update": "~8.1",
        "drupal/date": "8.*",
        "drupal/draggableviews": "~8.1",
        "drupal/ds": "~8.2",
        "drupal/entity": "~8.1",
        "drupal/entity_embed": "~8.1",
        "drupal/entity_browser": "~8.1",
        "drupal/field_group": "~8.1",
        "drupal/file_entity": "~8.2",
        "drupal/file_browser": "~8.1",
        "drupal/inline_entity_form": "~8.1",
        "drupal/maxlength": "~8.1",
        "drupal/media_entity": "~8.1",
        "drupal/media_entity_image": "~8.1",
        "drupal/pathauto": "~8.1",
        "drupal/redirect": "~8.1",
        "drupal/restui": "~8.1",
        "drupal/token": "~8.1",
        "drupal/views_bootstrap": "8.*",
        "drupal/recaptcha": "~8.2",
        "drupal/better_exposed_filters": "~8.3",
        "drupal/menu_link_attributes": "~8.",
        "drupal/eu_cookie_compliance": "~8.",
        "drupal/twig_field_value": "~8.1.1"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/drupal-extension": "~3.0",
        "drupal/devel": "~8.1",
        "drupal/masquerade": "~8.1",
        "drupal/coder": "~8.2.7",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "phing/phing": "~2.10",
        "drupal/phingdrushtask": "^1.1",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\DrupalProject\\": "tests/src",
            "DrupalProject\\": "src"
        },
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "htdocs/core": ["type:drupal-core"],
            "htdocs/libraries/{$name}": ["type:drupal-library"],
            "htdocs/modules/contrib/{$name}": ["type:drupal-module"],
            "htdocs/profiles/contrib/{$name}": ["type:drupal-profile"],
            "htdocs/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
